#include<stdio.h>
int main(){
	int sum=0,start,stop;
	printf("Enter start");
	scanf("%d",& start);
	
	printf("Enter stop");
	scanf("%d",&stop);
	
	do{printf("%d\n",start);
	sum=sum+start;
	sum+=start;
	start++;}
	
	while(start,start<=stop);
	
	printf("The sum%d",sum);
	
	return 0;
}