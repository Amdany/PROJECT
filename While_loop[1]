// while loop 
#include<stdio.h>

int main(){
  int start,stop,sum=0;
  
  printf("Enter where to start");
  scanf("%d",&start);
  
  printf("Enter where to stop");
  scanf("%d",&stop);
  
      while(start,start<=stop){
      printf ("%d\n",start);
      
	  start++;
	  sum=sum+start;
	  sum+=start; }
	printf("The sum is%d",sum);
	return 0;
	
	}